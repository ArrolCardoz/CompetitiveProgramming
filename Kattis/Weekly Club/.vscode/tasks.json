{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build with UCRT64 g++ (Git Bash)",
      "type": "shell",
      "command": "/c/msys64/ucrt64/bin/g++.exe",
      "args": [
        "-g",
        "-Wall",
        "-Wextra",
        "-O2",
        "-std=gnu++23",
        "${file}",
        "-o",
        // "${fileDirname}/${fileBasenameNoExtension}.exe"
        "${fileDirname}/a.exe"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}
